{
  "name": "@neon-rs/cli",
  "private": true,
  "version": "0.1.33",
  "description": "Command-line build tool for Neon modules.",
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "types/index.d.ts",
  "files": [
    "data/**/*",
    "dist/**/*",
    "types/**/*",
    "LICENSE*"
  ],
  "bin": {
    "neon": "dist/src/index.js"
  },
  "scripts": {
    "bundle": "bash bundle.sh",
    "update-cargo-messages": "bash update-cargo-messages.sh",
    "dist": "npm run update-cargo-messages && npm run bundle",
    "build": "tsc",
    "pretest": "npm run build",
    "prepack": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dherman/neon-rs.git"
  },
  "keywords": [
    "Neon",
    "Rust",
    "Node"
  ],
  "author": "Dave Herman <david.herman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dherman/neon-rs/issues"
  },
  "homepage": "https://github.com/dherman/neon-rs#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-commands": "^2.0.1",
    "@types/command-line-usage": "^5.0.2",
    "@types/jscodeshift": "^0.11.11",
    "@types/node": "^18.15.11",
    "@types/temp": "^0.9.1",
    "@vercel/ncc": "^0.36.1",
    "crlf": "^1.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@neon-rs/load": "^0.0.181",
    "@neon-rs/manifest": "^0.0.5",
    "cargo-messages": "^0.1.31",
    "chalk": "^5.2.0",
    "command-line-args": "^5.2.1",
    "command-line-commands": "^3.0.2",
    "command-line-usage": "^7.0.1",
    "execa": "^7.1.1",
    "jscodeshift": "^0.15.1",
    "stream-to-string": "^1.2.1",
    "temp": "^0.9.4"
  },
  "optionalDependencies": {
    "@cargo-messages/android-arm-eabi": "0.1.31",
    "@cargo-messages/darwin-arm64": "0.1.26",
    "@cargo-messages/darwin-x64": "0.1.26",
    "@cargo-messages/linux-arm-gnueabihf": "0.1.26",
    "@cargo-messages/linux-x64-gnu": "0.1.26",
    "@cargo-messages/win32-arm64-msvc": "0.1.26",
    "@cargo-messages/win32-x64-msvc": "0.1.26"
  }
}
